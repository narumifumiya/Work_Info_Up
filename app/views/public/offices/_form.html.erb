<%= form_with model: [company, office]  do |f| %>
        
  <div class="form-group row">
    <div class="col-md-3 mx-auto"><label for="name">事業所名</label></div>
    <div class="col-md-9"><%= f.text_field :name,  class: "form-control", id: "name" %></div>
  </div>
  
  <div class="form-group row">
    <div class="col-md-3 mx-auto"><label for="post_code">郵便番号（ハイフンなし）</label></div>
    <div class="col-md-9"><%= f.text_field :post_code,  class: "form-control", id: "post_code" %></div>
  </div>
  
  <div class="form-group row">
    <div class="col-md-3 mx-auto"><label for="address">住所</label></div>
    <div class="col-md-9"><%= f.text_field :address,  class: "form-control", id: "address" %></div>
  </div>
  
  <div class="form-group row">
    <div class="col-md-3 mx-auto"><label for="phone_number">電話番号</label></div>
    <div class="col-md-9"><%= f.text_field :phone_number,  class: "form-control", id: "phone_number" %></div>
  </div>
  
  <div class="row mt-3">
    <div class="col-md-3 mx-auto"></div>
    <div class="col-md-9"><%= f.submit "登録", class: "btn btn-sm btn-success"%></div>
  </div>

<% end %>