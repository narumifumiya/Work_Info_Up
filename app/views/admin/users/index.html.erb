<div class="container">
  <h2 class="text-center">メンバー</h2>
  <div class="row mt-5">
    <div class="col-11 col-md-12 mx-auto">
      <div>
        <%= link_to 'グループ一覧', admin_groups_path, class: 'text-dark' %>
      </div>
      <P>
        <%= link_to '新しい順', admin_users_path(latest: "true"), class: 'text-dark' %> |
        <%= link_to '古い順', admin_users_path(old: "true"), class: 'text-dark' %>
      </P>
      <%= render 'admin/users/index', users: @users %>
    </div>
    
    <div class="mx-auto mt-5"><%= paginate @users %></div>
  </div>
</div>